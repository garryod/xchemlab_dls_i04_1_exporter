{
    "compilerOptions": {
        "target": "ES2022",
        "lib": [
            "ES6"
        ],
        "module": "CommonJS",
        "resolveJsonModule": true,
        "allowJs": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "skipLibCheck": true
    },
}